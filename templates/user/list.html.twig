{% extends 'base.html.twig' %}

{% block title %}
    Inscrits
{% endblock %}

{% block body %}
    <h2 class="h2perso">Incrits</h2>
    <section class="user-list">
        {% for user in users %}
            <article>
                <a href="{{ path('user_show', {'id' : user.id}) }}">{{ user.username }}</a>
            </article>
        {% endfor %}
    </section>
    {% block header %}{% endblock %}
    {% block main %}{% endblock %}
    {% block footer %}{% endblock %}


{% endblock %}